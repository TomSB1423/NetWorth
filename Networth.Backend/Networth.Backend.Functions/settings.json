{
    "IsEncrypted": false,
    "Values": {
        "AzureWebJobsStorage": "UseDevelopmentStorage=true",
        "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated"
    },
    "Gocardless": {
        "BankAccountDataBaseUrl": "https://bankaccountdata.gocardless.com/api/v2"
    },
    "Host": {
        "CORS": "*"
    },
    "Database": {
        "Provider": "SQLite",
        "ConnectionStrings": {
            "SQLite": "Data Source=networth.db"
        },
        "AutoMigrate": true,
        "CommandTimeout": 30
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    }
}
