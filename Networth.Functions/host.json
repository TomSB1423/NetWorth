{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Debug"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "queues": {
            "visibilityTimeout": "00:01:00",
            "maxDequeueCount": 3
        }
    }
}
