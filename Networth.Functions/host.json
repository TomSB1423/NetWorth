{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Azure.Core": "Warning",
            "Azure.Storage": "Warning",
            "Npgsql": "Warning",
            "Microsoft.EntityFrameworkCore": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "queues": {
            "maxDequeueCount": 3
        }
    }
}
