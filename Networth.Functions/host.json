{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Microsoft.Azure.WebJobs.Host.Queues": "Warning",
            "Azure.Core": "Warning",
            "Host.Triggers.Queue": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "queues": {
            "maxDequeueCount": 3
        }
    }
}
