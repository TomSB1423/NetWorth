{
    "IsEncrypted": false,
    "Values": {
        "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated"
    },
    "Networth": {
        "UseAuthentication": false,
        "MockUser": {
            "FirebaseUid": "mock-user-123",
            "Name": "Mock Development User",
            "Email": "mock@example.com"
        }
    },
    "Gocardless": {
        "BankAccountDataBaseUrl": "https://bankaccountdata.gocardless.com/api/v2"
    },
    "Institutions": {
        "UseSandbox": true
    },
    "Serilog": {
        "Using": ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.AspNetCore": "Warning",
                "Microsoft.EntityFrameworkCore": "Warning",
                "Npgsql": "Warning",
                "Azure.Core": "Warning",
                "Azure.Storage": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    }
}
